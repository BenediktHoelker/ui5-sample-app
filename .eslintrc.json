{
	"env": {
		"node": true,
		"browser": true,
		"jasmine": true,
		"protractor": true,
		"jquery": true,
		"es6": true
	},
	"extends": ["airbnb", "prettier"],
	"plugins": ["prettier"],
	"globals": {
		"sap": "readonly",
		"QUnit": "readonly",
		"sinon": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2020
	},
	"rules": {
		"no-underscore-dangle": 0,
		"func-names": "off"
	}
}
